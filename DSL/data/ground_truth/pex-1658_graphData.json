    {
      "id": "b5816701-08e3-4805-9820-5865e7b93efc",
      "text": "add 1.666 \u00b5L of 5 mg/mL RNase A to the sample. Mix thoroughly",
      "instruction": "ADD",
      "slots": [
        {
          "key": "it's a slot key",
          "value": "it's a slot value",
          "id": "b5816701-08e3-4805-9820-5865e7b93efc",
