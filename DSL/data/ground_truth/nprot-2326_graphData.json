      "emits": []
    },
    {
      "id": "531f29f8-65a7-4e68-bbd5-7f450cb43d1d",
      "text": "Dispense 1 ml of stock \\(or desired amount) in a pre-labeled TrueCool cryovial",
      "instruction": "DISPENSE",
      "slots": [
        {
          "key": "it's a slot key",
          "value": "it's a slot value",
          "id": "531f29f8-65a7-4e68-bbd5-7f450cb43d1d",
