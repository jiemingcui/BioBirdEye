    {
      "id": "94065f05-8cd4-40a9-81ab-afbee4c60bc3",
      "text": "The CoolRackCFT30 reduces contamination and spill accidents by allowing one-handed opening/closing of the cryovials.",
      "instruction": "ADD",
      "slots": [
        {
          "key": "it's a slot key",
          "value": "it's a slot value",
          "id": "94065f05-8cd4-40a9-81ab-afbee4c60bc3",
