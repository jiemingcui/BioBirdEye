    {
      "id": "86083f87-d5e3-49f5-ad70-1a0bc3fb0561",
      "source": {
        "text": "DAY 4 and LATER",
        "node_id": "e81d7b39-07e3-4aa2-9d2d-9371736d44a1",
        "attr_id": "e81d7b39-07e3-4aa2-9d2d-9371736d44a1"
      },
      "target": {
        "text": "Aspirate media and replace with 18 mL of BEGM.",
        "node_id": "2b8e0ebf-96c5-469d-8266-ef25860159ac",
        "attr_id": "2b8e0ebf-96c5-469d-8266-ef25860159ac"
