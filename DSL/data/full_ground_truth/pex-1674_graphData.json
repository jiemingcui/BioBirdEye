      ],
      "emits": []
    },
    {
      "id": "b201b8ca-037d-47cd-b6e8-bf1da1e94b15",
      "text": "Carefully aspirate the supernatant.",
      "instruction": "ADD",
      "slots": [
        {
          "key": "it's a slot key",
          "value": "it's a slot value",
          "id": "b201b8ca-037d-47cd-b6e8-bf1da1e94b15",
