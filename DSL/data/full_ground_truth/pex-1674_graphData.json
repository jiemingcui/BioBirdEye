    },
    {
      "id": "f981f1db-1e71-4717-9b00-1c54e419f194",
      "text": " Continue to grow in Pneumacult Ex-Plus growth medium until the cells are confluent.",
      "instruction": "ADD",
      "slots": [
        {
          "key": "it's a slot key",
          "value": "it's a slot value",
          "id": "f981f1db-1e71-4717-9b00-1c54e419f194",
          "node_id": "f981f1db-1e71-4717-9b00-1c54e419f194"
        }
      ],
      "emits": []
    },
    {
      "id": "d69246c4-4540-435f-84d4-6037420f1e83",
      "text": "Once cells are confluent, withdraw the apical medium and replace the basolateral medium with Pneumacult ALI differentiation medium.",
      "instruction": "ADD",
      "slots": [
        {
          "key": "it's a slot key",
          "value": "it's a slot value",
          "id": "d69246c4-4540-435f-84d4-6037420f1e83",
          "node_id": "d69246c4-4540-435f-84d4-6037420f1e83"
        }
      ],
      "emits": []
    },
    {
      "id": "f75861e5-7855-4092-93f4-e704bf0fe302",
      "text": "Replace the Pneumacult ALI differentiation medium in the basolateral compartment every 48 hours",
      "instruction": "REPLACE",
      "slots": [
        {
          "key": "it's a slot key",
          "value": "it's a slot value",
          "id": "f75861e5-7855-4092-93f4-e704bf0fe302",
          "node_id": "f75861e5-7855-4092-93f4-e704bf0fe302"
        }
      ],
      "emits": []
    },
    {
      "id": "ad7d841f-2f07-494c-9d43-dc24d554f247",
      "text": "Wash the apical surface with DPBS at least once per week",
      "instruction": "ADD",
      "slots": [
        {
          "key": "it's a slot key",
