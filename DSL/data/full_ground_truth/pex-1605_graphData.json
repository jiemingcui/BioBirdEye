    },
    {
      "id": "07a979a5-8bfb-4f2a-a7c9-971ea12ff006",
      "text": "buffer A (50 mM Tris-HCl (pH 8),100 mM NaCl, 5% glycerol, 2 mM \u03b2- mercaptoethanol)",
      "instruction": "ADD",
      "slots": [
        {
          "key": "it's a slot key",
